            else {
                $("#header #logo img").attr("src", "https://www.bizweb.vn/Themes/Portal/Default/Images/bizweb_old_logo_short.png");
                $("#header .small-logo img").attr("src", "https://www.bizweb.vn/Themes/Portal/Default/Images/bizweb_old_logo_short.png");
            }
        }
        else {
            if ($("#header").hasClass("index") || $("#header").hasClass("aqua")) {
                $("#header #logo img").attr("src", "https://www.bizweb.vn/Themes/Portal/Default/Images/newlogo/bizweb_logo_new.svg");
